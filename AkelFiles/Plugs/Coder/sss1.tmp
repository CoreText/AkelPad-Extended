Quotes:

QuotesRE:
;для поиска
1 `(?=\b|\W)%#$&@(?=\W|\b)` "\0=(0,#000000,#00FF00)"
2 `(?=\b|\W)%#$&@(?=\W|\b)` "\0=(0,#FFFFFF,#FF00FF)"
1 `%#$&@` "\0=(0,#FFFFFF,#FF0080)"
2 `%#$&@` "\0=(0,#FFFFFF,#8000ff)"
;cmd path>
1 "(^\w\:\\.+?\>)(.*?)$" "\1=(0,#00FF00,#000000) \2=(0,#FFFF00,#000000)"
;find_str.js AkelPad
1 "^----------\s.+$" "\0=(0,#ff0080,#000000)"
1 "^\[\d+\]" "\0=(0,${COMM},0)"
1 `^\+\+\+.*?$` "\0=(0,#00ff00,0)"
1 `^\-\-\-.*?$` "\0=(0,#ff0080,0)"
;Для FindReplaceEx.js
1 "^  \[\d+, \d+, \d+, \d+:\d+\]" "\0=(0,${COMM},0)"
1 "\A[A-Z]:\\.+?:" "\0=(0,${DEL1},0)"
1 "Всего вхождений: \d+\Z" "\0=(0,${DEL2},0)"
1 "Total found: \d+\Z" "\0=(0,${DEL2},0)"
;Для SearchReplace_extended.js
1 "\(\d+:\d+\)" "\0=(0,${COMM},0)"
1 "\A\d+:" "\0=(0,${DEL1},0)"
;QSearch
1 '^Search(ing|ed)? .*?:?$' "\0=(0,${DEL1},0)"
1 "^\s\(\d*\s?\d+\:?\d+\)(?:\s*)?" "\0=(0,${COMM},0)"
1 "Total\s.*?$" "\0=(0,${TAG},0)"
;findstr \O
;1 "^(.*)+(\:\d+\:\d+)(\:)" "\1=(0,#FFFF80,#000000) \2=(0,${COMM},#000000) \3=(0,#000000,#000000)"
;findstr
1 "^(.*)+(\:\d+)(\:)" "\1=(0,#FFFF80,#000000) \2=(0,${COMM},#000000) \3=(0,#000000,#000000)"
;1 "in file" "\0=(0,${DEL1},0)"
;Для HTMLTidy
;1 "^line \d+ column \d+" "\0=(0,#AFE7B3,0)"
1 "^(line) (\d+) (column) (\d+) (\-)" "\1=(0,${COMM},0) \2=(0,${NUM},0) \3=(0,${COMM},0) \4=(0,#${COMM},0) \5=(0,${COMM},0)"
;0 '[а-яё]' "\0=(0,#BFCAA9,0)"
;Для HTML
1	' (\w+?)(=)("[^"<>]+")'	"\1=(0,${ATTR},0)	\2=(0,${DEL2},0) \3=(0,${STR},0)" 3 0
1	' (\w+?)(=)(\d+)'	"\1=(0,${ATTR},0)	\2=(0,${DEL2},0) \3=(0,${NUM},0)" 3 0
;Для путей
;1 '\b[A-Z]:(?:\\[^\\/:\*\?"<>\|]+?)+' "\0=(0,#FF0080,0)"
;Для предупреждений
0	'\berror'	"\0=(0,#FF0031,0)"
0	'\bnotice'	"\0=(0,${TAG},0)"
0	'\bwarning'	"\0=(0,${DEL2},0)"
;Functions/Methods
0	'\bdef ' "\0=(0,${TAG},0)"
0	'\bfn' "\0=(0,${TAG},0)"
0	'\bfunc ' "\0=(0,${TAG},0)"
0	'\bfunction ' "\0=(0,${TAG},0)"
;0 '[а-яё]' "\0=(0,${COMM},0)"
;Операторы
;0 '[&+=*\(\)\{\}\]\[]' "\0=(0,#004040,0)"
;Латинский алфавит
;0 '[a-zA-Z]' "\0=(0,#AAA6DB,0)" ;может C0AD72
;0 '[^a-zA-Z]' "\0=(0,#C0C0C0,0)" ;может C0AD72
;0 '[a-zA-Z]' "\0=(0,${DEL2},0)"
;0 '[^a-zA-Z]' "\0=(0,${DEL2},0)"
;Числа
;0 "\d" "\0=(0,#EBCB9C,0)"
;Текст в кавычках
;0 '".+?"' "\0=(0,${STR},0)"
;0 "'.+?'" "\0=(0,${STR},0)"

